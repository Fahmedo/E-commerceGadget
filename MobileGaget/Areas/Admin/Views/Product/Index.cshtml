@model IEnumerable<MobileGadget.Model.Product>




    <div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary"></h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Product" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-square"></i> &nbsp; Create New Product
            </a>
        </div>
    </div>
</div>
<br />
<br />

<table id="tblData" class="table table-boardered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Price</th>
            <th>About</th>
            <th></th>


        </tr>
    </thead>
</table>
